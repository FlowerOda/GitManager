{
  "name": "git-rollback-extension",
  "displayName": "Git Rollback Extension",
  "description": "在右键菜单中添加 Git Rollback 功能，执行 'git reset --hard HEAD^' 回滚到上个版本。",
  "version": "0.0.1",
  "publisher": "花小田",
  "engines": {
    "vscode": "^1.62.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
   
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.gitRollback",
        "title": "Git Rollback"
      }
    ],
    "submenus": [
      {
        "id": "extension.gitSubmenu",
        "label": "Git"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "submenu": "extension.gitSubmenu"
        
        }
      ],
      "extension.gitSubmenu": [
        {
          "command": "extension.gitRollback"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "^14.14.37",
    "@types/vscode": "^1.62.0",
    "typescript": "^4.4.3",
    "vscode-test": "^1.6.2"
  }
}
